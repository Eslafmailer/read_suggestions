{
  "name": "scrapper",
  "version": "1.0.0",
  "dependencies": {
    "@types/axios": "0.14.0",
    "@types/circular-json": "0.4.0",
    "@types/lodash": "4.14.191",
    "@types/node": "18.14.6",
    "axios": "1.3.4",
    "cheerio": "1.0.0-rc.12",
    "circular-json": "0.5.9",
    "lodash": "4.17.21",
    "moment": "2.29.4",
    "random-words": "1.3.0",
    "serialize-error": "8.1.0",
    "tslib": "2.5.0",
    "type-fest": "3.6.1",
    "typescript": "4.9.5"
  },
  "scripts": {
    "scrap": "tsc && node scrap.js",
    "scrap:debug": "tsc && node --inspect-brk scrap.js",
    "label": "tsc && node label.js",

    "embedd:bovw": "poetry run python bovw.py",
    "add-cover": "tsc && node add-cover.js",

    "embedd:cnn:resnet18": "poetry run python cnn_resnet18.py",
    "embedd:cnn:densenet": "poetry run python cnn.py --model 'densenet'",
    "embedd:cnn:efficientnet": "poetry run python cnn.py --model 'efficientnet_b7'",

    "embedd:cnn:custom:prepare": "tsc && node label-images.js",
    "embedd:cnn:custom:train": "poetry run python vgg_train.py",
    "postembedd:cnn:custom:train": "rm -rf images",
    "embedd:cnn:custom:predict": "poetry run python vgg_run.py",

    "anonymize": "tsc && node anonymize.js",
    "accept": "tsc && node accept.js",
    "validate": "tsc && node validate.js",

    "static:images": "tsc && node label-images.js && mv images/true/ static/images/ && rm -rf images",
    "static:script": "tsc && node static.js",
    "static:publish": "surge ./static && rm -rf static/images && rm -rf static/books.js"
  }
}
